    <td><%= hwm_studio.reg_addr %></td>
    <td><%= hwm_studio.establish_date %></td>
    <td><%= hwm_studio.due_date %></td>
    <td><%= hwm_studio.organize_code %></td>
    <td><%= hwm_studio.tax_reg_cert %></td>
    <td><%= hwm_studio.verify_state %></td>
    <td><%= hwm_studio.verify_reason %></td>
    <td><%= link_to 'Show', hwm_studio %></td>
    <td><%= link_to 'Edit', edit_hwm_studio_path(hwm_studio) %></td>
    <td><%= link_to 'Destroy', hwm_studio, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

