<% @hwm_studios.each do |hwm_studio| %>
  <tr>
    <td><%= hwm_studio.hwm_user_account_id %></td>
    <td><%= hwm_studio.auth_type %></td>
    <td><%= hwm_studio.tag %></td>
    <td><%= hwm_studio.industry %></td>
    <td><%= hwm_studio.nature %></td>
    <td><%= hwm_studio.studioname %></td>
    <td><%= hwm_studio.studio_intro %></td>
    <td><%= hwm_studio.studio_desc %></td>
    <td><%= hwm_studio.country %></td>
    <td><%= hwm_studio.province %></td>
    <td><%= hwm_studio.city %></td>
    <td><%= hwm_studio.district %></td>
    <td><%= hwm_studio.address %></td>
    <td><%= hwm_studio.tel %></td>
    <td><%= hwm_studio.email %></td>
    <td><%= hwm_studio.fax %></td>
    <td><%= hwm_studio.corp_repr %></td>
